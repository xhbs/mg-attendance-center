	<% if(context.env === "development") { %>
        <%- `
            <script>
              window.hzConfig = ${JSON.stringify(context.hzConfig)}
              document.cookie = "username=admin;expires="  +new Date(4070880000000).toGMTString()
              document.cookie = "usercode=admin;expires="  +new Date(4070880000000).toGMTString()
            </script>
        `%>
    <% }else if(context.env === "production"){ %>
        <script>
            /* apollo配置参数注入update */
            window.hzConfig = <$- JSON.stringify(config) $>
        </script>
    <% } %>

</head>

<body class="hz-base-style">
