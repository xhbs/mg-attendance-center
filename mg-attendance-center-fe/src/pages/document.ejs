<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="content-type" content="text/html; charset=utf-8">
    <meta name="renderer" content="webkit">
    <meta name="force-rendering" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport"
        content="width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1, user-scalable=no" />
    <meta name="referrer" content="always">

	<script type="text/babel" src="../../node_modules/babel-polyfill/dist/polyfill.js"></script>
    <script type="text/babel" src="../../node_modules/babel-polyfill/browser.js"></script>

    <!-- socket -->
    <% if (process.env.NODE_ENV==='development') { %>
        <script src="/sockjs.min.js"></script>
        <script src="/stomp.min.js"></script>
    <% }else if(process.env.NODE_ENV==='production'){ %>
        <script src="/sceneAttendance/sockjs.min.js"></script>
        <script src="/sceneAttendance/stomp.min.js"></script>
    <% } %>

    <!-- vms chrome视频播放插件 -->
    <% if (process.env.NODE_ENV==='development') { %>
        <%- `
        <script type="text/javascript" src="/video/react.development.js"></script>
        <script type="text/javascript" src="/video/react-dom.development.js"></script>
        <!-- <script type="text/javascript" src="/bclient/commonresource/vplay.js"></script> -->
        `%>
    <% }else if(process.env.NODE_ENV==='production'){ %>
        <%- `
        <!-- <script type="text/javascript" src="/bclient/commonresource/vplay.js"></script> -->
        `%>
    <% } %>

    <!-- ugs -->
